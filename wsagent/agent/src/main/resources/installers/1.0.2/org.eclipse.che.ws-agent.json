{
  "id": "org.eclipse.che.ws-agent",
  "version": "1.0.2",
  "name": "Workspace API",
  "description": "Workspace API support",
  "dependencies": [
    "org.eclipse.che.exec",
    "org.eclipse.che.terminal"
  ],
  "properties": {},
  "servers": {
    "wsagent/http": {
      "port": "4401/tcp",
      "protocol": "http",
      "path" : "/api",
      "attributes": {
        "secure": "true",
        "unsecuredPaths": "/api/liveness"
      }
    },
    "wsagent/ws": {
      "port": "4401/tcp",
      "protocol": "ws",
      "path" : "/wsagent",
      "attributes": {
        "secure": "true",
        "unsecuredPaths": "/api/liveness"
      }
    }
  }
}
